<view><block wx:if="{{danxuan}}"><danxuanpage vue-id="1" questionList="{{questionList}}" number="{{number}}" qNumber="{{qNumber}}" tag="{{tag}}" nextText="{{nextText}}" jiexiTag="{{jiexiTag}}" data-event-opts="{{[['^func',[['changeQuestion']]]]}}" bind:func="__e" bind:__l="__l"></danxuanpage></block><block wx:if="{{duoxuan}}"><duoxuanpage vue-id="2" questionList="{{questionList}}" number="{{number}}" qNumber="{{qNumber}}" tag="{{tag}}" nextText="{{nextText}}" jiexiTag="{{jiexiTag}}" data-event-opts="{{[['^func',[['changeQuestion']]]]}}" bind:func="__e" bind:__l="__l"></duoxuanpage></block><block wx:if="{{panduan}}"><panduanpage vue-id="3" questionList="{{questionList}}" number="{{number}}" qNumber="{{qNumber}}" tag="{{tag}}" nextText="{{nextText}}" jiexiTag="{{jiexiTag}}" data-event-opts="{{[['^func',[['changeQuestion']]]]}}" bind:func="__e" bind:__l="__l"></panduanpage></block><block wx:if="{{jianda}}"><jiandapage vue-id="4" questionList="{{questionList}}" number="{{number}}" qNumber="{{qNumber}}" tag="{{tag}}" nextText="{{nextText}}" jiexiTag="{{jiexiTag}}" data-event-opts="{{[['^func',[['changeQuestion']]]]}}" bind:func="__e" bind:__l="__l"></jiandapage></block><view class="line"></view><view class="cmt-container"><view class="cmt-title">发表笔记</view><view class="cmt-content"><textarea value="{{biji}}" placeholder="请输入您的笔记(最多120字)" maxlength="120" data-event-opts="{{[['input',[['__set_model',['','biji','$event',[]]]]]]}}" bindinput="__e"></textarea></view><button type="primary" data-event-opts="{{[['tap',[['postBiji',['$event']]]]]}}" bindtap="__e">发表</button><block wx:for="{{questionList[qNumber].bijiList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cmt-comments"><view class="cmt-header"><view class="cmt-avatar"><image src="{{item.avatarUrl}}"></image></view><view class="cmt-id-time"><view class="cmt-id">{{''+item.nickName+''}}</view><view class="cmt-time">{{''+item.time+''}}</view></view><view class="cmt-collection">收藏<image src="../../static/星星1.png"></image></view></view><view class="cmt-bottom">{{''+item.content+''}}</view><view class="cmt-line"></view></view></block></view></view>