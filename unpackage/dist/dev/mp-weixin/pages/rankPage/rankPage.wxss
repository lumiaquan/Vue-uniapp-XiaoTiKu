
.rank-top-container{
	width:100%;
	height:144px;
	background:rgba(64,158,255,1);
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin-bottom: 26px;
}
.rank-top-img{
	margin-left: -40px;
	margin-right: 21px;
}
.rank-top-img image{
	width: 61px;
	height: 61px;
}
.rank-top-line{
	width:1px;
	height:56px;
	background-color: #247FE0;
	margin-right: 20px;
}
.rank-top-text{
}
.text1{
	font-size:20px;
	font-family:STHeitiSC-Medium,STHeitiSC;
	font-weight:500;
	color:rgba(255,255,255,1);
	line-height:20px;
	margin-top: -5px;
	margin-bottom: 3px;
}
.text2{
	width: 170px;
	font-size:12px;
	font-family:STHeitiSC-Medium,STHeitiSC;
	font-weight:400;
	color:rgba(255,255,255,1);
	line-height:14px;
}
.rank-middle-container{
}
.rank-middle{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	position: relative;
	margin-bottom: 23px;
}
.rank-number{
	width:33px;
	height:33px;
	background:rgba(211,76,74,1);
	border-radius: 50%;
	color: #FFFFFF;
	font-size:16px;
	font-family:STHeitiSC-Medium,STHeitiSC;
	font-weight:500;
	line-height: 33px;
	text-align: center;
	margin-left: 12%;
	margin-right: 31px;
}
.rank-name{
	font-size:16px;
	font-family:STHeitiSC-Medium,STHeitiSC;
	font-weight:500;
	color:rgba(0,0,0,1);
	line-height:33px;
}
.rank-stars{
	position: absolute;
	right: 12%;
}
.rank-stars image{
	width: 14px;
	height: 14px;
	margin-right: 5px;
}
.rank-stars text{
	font-size:16px;
	font-family:STHeitiSC-Medium,STHeitiSC;
	font-weight:500;
	color:rgba(0,0,0,1);
	line-height:33px;
}

