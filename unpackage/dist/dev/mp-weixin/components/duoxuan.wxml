<view singlechoice-container><view class="Header"><view class="number">{{''+number+''}}</view><view class="category">多选题</view></view><view class="question-content">{{''+content+''}}</view><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="Eindex"><view class="choices-container"><view class="choices-subcontainer"><view data-event-opts="{{[['tap',[['activeChoice',[index]]]]]}}" class="{{[item.g0?'choices-content-active':'choices-content ']}}" bindtap="__e">{{''+item.$orig+''}}</view></view></view></block><block wx:if="{{jiexiTag==1}}"><view class="jiexi-container"><view class="jiexi-title">正确答案:<block wx:for="{{rightAnswer}}" wx:for-item="item" wx:for-index="index" wx:key="*this"><text>{{item}}</text></block></view></view></block><block wx:if="{{jiexiTag==1}}"><view class="jiexi-container"><view class="jiexi-title">解析:</view><view class="jiexi">{{''+jiexi+''}}</view></view></block><view class="pre-container"><view data-event-opts="{{[['tap',[['changeQuestion',[-1]]]]]}}" class="{{[number==1?'pre2':'pre']}}" bindtap="__e"><view class="{{[number==1?'pre-text2':'pre-text']}}">上一题</view></view></view><view class="next-container"><view data-event-opts="{{[['tap',[['changeQuestion',[1]]]]]}}" class="next" bindtap="__e"><view class="next-text">{{''+nextText+''}}</view></view></view></view>